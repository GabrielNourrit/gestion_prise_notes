SET foreign_key_checks=0;

CREATE TABLE matiere(
nom varchar(30),
surnom varchar(15),
semestre tinyint,
type varchar(4)

)ENGINE=InnoDb;
                               
CREATE TABLE Management_SI(
id int auto_increment,
contenu longtext,
primary key(id)
)ENGINE=InnoDb;
CREATE TABLE Management_SI_TD(
                            id int auto_increment,
                               contenu longtext,
                               primary key(id)
                               )ENGINE=InnoDb;                               
